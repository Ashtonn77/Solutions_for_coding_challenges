   std::vector<int> arr {2, 5, 6, 6, 7, 1, 2, 5};
    std::vector<int> newArr;
    std::set<int> num;

    for(auto x:arr){
    num.insert(x);
    }
   std::set<int>::iterator it = num.begin();

   for(; it != num.end(); it++){
    newArr.push_back(*it);
   }

   for(auto x:newArr){
    std::cout << x << std::endl;
    }


