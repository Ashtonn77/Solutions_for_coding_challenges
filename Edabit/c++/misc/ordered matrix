one -->

std::vector<std::vector<int> > orderedMatrix(int a, int b) {
	  int sum{0};
    std::vector<std::vector<int> > A;
    std::vector<int> B;
        for(int i = 1; i <= a; i++){
          for(int j = 1; j <= b; j++){
            sum++;
            B.push_back(sum);
          }
            A.push_back(B);
	    B = {};
        }
    return A;
}

