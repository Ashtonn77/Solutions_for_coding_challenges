void decentNumber(int n) {

   int three = 0;
   int five = 0;
   
   while(n > 0){
       
       if(n % 3 == 0){
           
           five = n;
           break;
           
       }
       else if(n <= 4){

           cout << "-1" << endl;
           return;
           
       }
       else{
           
           three += 5;
           n -= 5;
           
       }
       
   }
   
   string f(five, '5');
   string t(three, '3');
   
   cout << f + t << endl;
}

